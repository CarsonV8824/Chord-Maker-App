<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.SecondaryController" stylesheets="@secondary.css">
    <children>
        <VBox alignment="TOP_LEFT" spacing="10.0">
            <Button fx:id="primaryButton" text="Generate Chords" onAction="#switchToPrimary"/>
        </VBox>
        <HBox alignment="TOP_CENTER" spacing="20.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
            <ListView fx:id="savedChordsListView" prefHeight="200.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" />
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
